    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.2",
    "@types/pino": "^7.0.4",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
