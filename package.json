{
  "name": "graphnode_be",
  "version": "1.0.0",
  "description": "TACO 4th Team Project - GraphNode",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest",
    "docs:lint": "npx @stoplight/spectral lint -r .spectral.yaml docs/api/openapi.yaml docs/schemas/*.json",
    "docs:openapi:build": "npx @redocly/cli build-docs docs/api/openapi.yaml -o docs/api/openapi.html",
    "docs:typedoc": "typedoc --options typedoc.json",
    "docs:copy:changelog": "node -e \"require('fs').cpSync('CHANGELOG.md','docs/CHANGELOG.md',{recursive:false})\"",
    "docs:build": "npm run docs:openapi:build && npm run docs:typedoc && npm run docs:copy:changelog",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down -v",
    "db:logs": "docker compose logs -f",
    "test:users": "tsx tests/scripts/test-users-crud.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TACO-FOR-ALL/GraphNode_BE.git"
  },
